 FormBox[
  RowBox[{"S", "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      SubscriptBox["D", "1"], 
      RowBox[{"r", "-", "g"}]], " ", "\[DoubleLongRightArrow]", " ", "r"}], 
    "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["D", "1"], "+", 
      RowBox[{"S", " ", "g"}]}], "S"]}]}], TraditionalForm]], "Equation",
