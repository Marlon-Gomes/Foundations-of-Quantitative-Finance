  RowBox[{
   RowBox[{"1.2", " ", "5"}], ",", "0.12", ",", "0.20", ",", "5", ",", 
   "0.03"}], "]"}]], "Input",
 CellChangeTimes->{{3.587142054674508*^9, 3.587142058466042*^9}, {
   3.5871421138416758`*^9, 3.5871421538988743`*^9}, {3.587142453902082*^9, 
   3.587142462781063*^9}, {3.587155738336021*^9, 3.58715573862368*^9}, 
   3.587156176883195*^9},
