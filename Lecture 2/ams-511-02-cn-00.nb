Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"vnSensitivityAnalysis", "=", 
  RowBox[{"Table", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"xCons", "=", 
      RowBox[{"And", "@@", "\[IndentingNewLine]", 
       RowBox[{"Join", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"vnCosts", ".", "vxChoices"}], "\[LessEqual]", 
           "nCapital"}], "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"0", "\[LessEqual]", "#", "\[LessEqual]", "1"}], "&"}], "/@",
           "vxChoices"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"#", "\[Element]", "Integers"}], "&"}], "/@", 
          "vxChoices"}]}], "\[IndentingNewLine]", "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"vxSolution", "=", 
      RowBox[{"Maximize", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"xObj", ",", "xCons"}], "}"}], ",", "vxChoices"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"viOptimalSolution", "=", 
      RowBox[{"vxChoices", "/.", 
       RowBox[{"Last", "[", "vxSolution", "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"nCapital", ",", 
       RowBox[{"First", "[", "vxSolution", "]"}], ",", "viOptimalSolution", 
       ",", "viOptimalSolution"}], "}"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"nCapital", ",", "400", ",", "700", ",", "20"}], "}"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
