Cell["\<\
The internal rate of return, IRR, is that interest rate consistent with a PV \
of 0 for a given set of cash flows. Consider a case where the cash flows are \
received at each compounding interval, then\
\>", "Text",
 CellChangeTimes->{{3.5856182845177193`*^9, 3.5856183125630293`*^9}, {
  3.5856183537168713`*^9, 3.5856184001127243`*^9}, {3.58561945953363*^9, 
  3.585619520663809*^9}, {3.585619682780744*^9, 3.585619684540587*^9}, {
  3.585803993101245*^9, 3.585804025298037*^9}, {3.713101619706689*^9, 
  3.713101620037429*^9}, {3.713102358537579*^9, 
  3.713102363465475*^9}},ExpressionUUID->"fd8031d4-e8cf-47cc-a45f-\
c44b72f3d382"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"IRR", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["c", "0"], ",", 
        SubscriptBox["c", "1"], ",", "\[Ellipsis]", ",", 
        SubscriptBox["c", 
         RowBox[{"n", "\[Times]", "k"}]]}], "}"}], " ", "|", " ", "k"}], ",", 
     " ", "n"}], "]"}], "=", 
   RowBox[{
    UnderscriptBox["solve", "y"], " ", "[", 
    RowBox[{"0", "=", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "0"}], 
       RowBox[{"n", "\[Times]", "k"}]], 
      FractionBox[
       SubscriptBox["c", "i"], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"y", "/", "k"}]}], ")"}], "i"]]}]}], "]"}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.585615242824792*^9, 3.585615327692699*^9}, 
   3.585615781721428*^9, {3.585615812140017*^9, 3.585615820867283*^9}, {
