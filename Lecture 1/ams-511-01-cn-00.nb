  RowBox[{
   UnderscriptBox["solve", "p"], " ", "[", 
   RowBox[{"B", "=", 
    RowBox[{"p", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], 
       RowBox[{"n", " ", "\[Times]", " ", "k"}]], 
      FractionBox["1", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"r", "/", "k"}]}], ")"}], "i"]]}]}]}], "]"}], 
  TraditionalForm]], "Equation",
