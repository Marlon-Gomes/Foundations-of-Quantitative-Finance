  RowBox[{
   RowBox[{
    UnderscriptBox["lim", 
     RowBox[{"k", "\[Rule]", "\[Infinity]"}]], 
    RowBox[{"[", 
     RowBox[{"PV", " ", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"r", "/", "k"}]}], ")"}], 
       RowBox[{"n", " ", "\[Times]", " ", "k"}]]}], "]"}]}], "=", 
   RowBox[{"PV", " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"n", " ", "r"}]]}]}], TraditionalForm]], "Equation",
