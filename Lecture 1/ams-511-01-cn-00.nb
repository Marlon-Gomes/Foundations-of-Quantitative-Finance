Cell[TextData[{
 StyleBox["Mathematical Economics",
  FontSlant->"Italic"],
 " (",
 ButtonBox["http://en.wikipedia.org/wiki/Mathematical_economics",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Mathematical_economics"], None},
  ButtonNote->"http://en.wikipedia.org/wiki/Mathematical_economics"],
