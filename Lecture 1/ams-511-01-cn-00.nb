 FormBox[
  RowBox[{"S", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], 
       RowBox[{"n", " ", "\[Times]", " ", "k"}]], 
      FractionBox["c", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"y", "/", "k"}]}], ")"}], "i"]]}], "+", 
     FractionBox["F", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"y", "/", "k"}]}], ")"}], 
       RowBox[{"n", " ", "\[Times]", " ", "k"}]]]}], "=", 
    RowBox[{
     StyleBox[
      RowBox[{"k", " ", 
       FractionBox["c", "y"], 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         FractionBox["1", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"1", "+", 
             RowBox[{"y", "/", "k"}]}], 
            StyleBox[")",
             FontSlant->"Italic"]}], 
           RowBox[{"n", " ", "\[Times]", " ", "k"}]]]}], ")"}]}],
      FontColor->RGBColor[0, 0, 1]], "+", 
     StyleBox[
      FractionBox["F", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"y", "/", "k"}]}], ")"}], 
        RowBox[{"n", " ", "\[Times]", " ", "k"}]]],
      FontColor->RGBColor[1, 0, 0]]}]}]}], TraditionalForm]], "Equation",
