20247db04767"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"FV", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["c", "0"], ",", 
        SubscriptBox["c", "1"], ",", "\[Ellipsis]", ",", 
        SubscriptBox["c", 
         RowBox[{"n", "\[Times]", "k"}]]}], "}"}], " ", "|", " ", "r"}], ",", 
     " ", "k", ",", " ", "n"}], "]"}], "=", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"i", "=", "0"}], 
     RowBox[{"n", "\[Times]", "k"}]], 
    SuperscriptBox[
     RowBox[{
      SubscriptBox["c", "i"], "(", 
      RowBox[{"1", "+", 
       RowBox[{"r", "/", "k"}]}], ")"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "\[Times]", "k"}], ")"}], "-", "i"}]]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.585614932582492*^9, 
  3.585615081342216*^9}},ExpressionUUID->"a4216126-bde9-4931-a768-\
e3ad85f8beee"]
