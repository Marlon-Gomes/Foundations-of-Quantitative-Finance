Cell[TextData[{
 "Consider investing ",
 StyleBox["F",
  FontSlant->"Italic"],
 " for ",
 StyleBox["n",
  FontSlant->"Italic"],
 " years at rate ",
 StyleBox["r",
  FontSlant->"Italic"],
 " at frequency ",
 StyleBox["k",
  FontSlant->"Italic"],
 ". Computing the limit as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", " ", "\[Rule]", " ", "\[Infinity]"}], TraditionalForm]],
  ExpressionUUID->"7340c0c5-ee5d-4e6d-9648-773fd92abaa1"],
 " results in the exponential function:"
}], "Text",
 CellChangeTimes->{{3.585544960834827*^9, 3.585544985343651*^9}, {
