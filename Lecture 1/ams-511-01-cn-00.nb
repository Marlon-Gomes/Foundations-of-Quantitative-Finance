  FontSlant->"Italic"],
 StyleBox[" today one must pay (",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox["F",
  FontSize->16,
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox[" + ",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox["r",
  FontSize->16,
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox[" \[Times] ",
