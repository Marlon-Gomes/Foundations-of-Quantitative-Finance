   RowBox[{
    SubscriptBox["\[Rho]", 
     RowBox[{"i", ",", "j"}]], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Sigma]", 
       RowBox[{"i", ",", "j"}]], "/", 
      SubscriptBox["\[Sigma]", "i"]}], 
