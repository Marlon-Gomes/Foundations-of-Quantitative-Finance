Cell[TextData[{
 "The Value-at-Risk or VaR (See ",
 ButtonBox["http://en.wikipedia.org/wiki/Value_at_risk",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Value_at_risk"], None},
  ButtonNote->"http://en.wikipedia.org/wiki/Value_at_risk"],
 ".) is the magnitude of the loss expected over a given time period at a \
given confidence level.  Note there are two parameters to the VaR: the time \
period \[Delta] and the confidence level \[Chi]. If ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", "\[Delta]"], TraditionalForm]],ExpressionUUID->
  "0b4f9e6b-5adc-4991-828f-197d707772b2"],
 " is the CDF of returns over a period of length \[Delta], then the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["VaR", 
    RowBox[{"\[Delta]", ",", "\[Chi]"}]], TraditionalForm]],ExpressionUUID->
  "65412edd-1235-44c4-8785-286b3e6ba62f"],
 " is"
}], "Text",
 CellChangeTimes->{{3.4325016658338346`*^9, 3.432501709311075*^9}, {
   3.4325018350753613`*^9, 3.432501840970372*^9}, {3.4325018804069147`*^9, 
   3.432501921404419*^9}, {3.432502016489791*^9, 3.4325020334652843`*^9}, {
   3.432502105673698*^9, 3.432502168034812*^9}, {3.432502397179491*^9, 
   3.432502431963089*^9}, {3.432502575697135*^9, 3.432502602883788*^9}, {
   3.432560440989587*^9, 3.432560447261937*^9}, {3.4325729439009333`*^9, 
   3.432572990499135*^9}, 3.606436953148419*^9, {3.606437164928618*^9, 
   3.606437191735824*^9}, {3.606437794979574*^9, 3.606437834650765*^9}, 
   3.606438789265608*^9, {3.6064388199625177`*^9, 3.60643883895124*^9}, {
   3.606438910846221*^9, 3.606438948133617*^9}, {3.606439574304788*^9, 
   3.606439611807962*^9}, {3.606453509066318*^9, 
   3.606453556841434*^9}},ExpressionUUID->"43fba230-4e66-498e-a909-\
cc95ce28fec6"],

Cell[BoxData[
 RowBox[{
  SubscriptBox["VaR", 
   RowBox[{"\[Delta]", ",", " ", "\[Chi]"}]], "=", 
  RowBox[{
   UnderscriptBox["argmin", 
    StyleBox["r",
     FontSlant->"Italic"]], "[", 
   RowBox[{
    RowBox[{"1", "-", 
     RowBox[{
      SubscriptBox[
       StyleBox["F",
        FontSlant->"Italic"], "\[Delta]"], 
      RowBox[{"(", 
       StyleBox["r",
        FontSlant->"Italic"], ")"}]}]}], " ", "\[LessEqual]", " ", 
    RowBox[{"1", "-", "\[Chi]"}]}], "]"}]}]], "Equation",
 CellChangeTimes->{{3.431983050071645*^9, 3.4319830553212233`*^9}, {
   3.4319831721937838`*^9, 3.431983183684338*^9}, {3.4319833314256773`*^9, 
