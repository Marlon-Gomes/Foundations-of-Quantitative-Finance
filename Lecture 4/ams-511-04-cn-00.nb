Cell[TextData[{
 "Given the values of the returns ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "M"], TraditionalForm]],ExpressionUUID->
  "a9ed0af5-6825-4353-a7ef-7b3b63f303c4"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "S"], TraditionalForm]],ExpressionUUID->
  "b487cfae-3ce0-414b-b144-3159e699f843"],
 ", and ",
 Cell[BoxData[
