41303f28f6a0"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[DoubleStruckCapitalE]", "[", 
    SuperscriptBox["X", "n"], "]"}], "=", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
     RowBox[{
      SuperscriptBox["x", "n"], " ", 
      RowBox[{"f", "[", 
       RowBox[{"x", "|", "\[Nu]"}], "]"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], 
    OverscriptBox["\[Proportional]", "~"], 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
     RowBox[{
      SuperscriptBox["x", 
       RowBox[{"n", "-", 
        RowBox[{"(", 
         RowBox[{"1", "+", "\[Vee]"}], ")"}]}]], 
      RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], TraditionalForm]], "Equation",\

 CellChangeTimes->{{3.8138791631445847`*^9, 3.813879274576943*^9}, {
