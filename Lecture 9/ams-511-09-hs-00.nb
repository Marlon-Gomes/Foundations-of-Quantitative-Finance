b2b3f9b9b122"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubsuperscriptBox["b", "Kelly", "*"], "=", 
   RowBox[{
    UnderscriptBox["argmax", "b"], "[", 
    RowBox[{
     RowBox[{"p", " ", 
      RowBox[{"log", "[", 
       RowBox[{"w", "+", "b"}], " ", "]"}]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", "p"}], ")"}], " ", 
      RowBox[{"log", "[", 
       RowBox[{"w", "-", "b"}], "]"}]}]}], "]"}]}], 
  TraditionalForm]], "Equation",
