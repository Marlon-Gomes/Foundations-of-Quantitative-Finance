Cell[TextData[{
 "The ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["n", "th"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "371faed8-25b2-4803-a9a5-233750c97e43"],
 " moment is"
}], "Text",
 CellChangeTimes->{{3.8138791414623127`*^9, 3.813879154129822*^9}, {
