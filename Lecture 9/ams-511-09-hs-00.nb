3aa3f9d7181e"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     SubscriptBox["\[PartialD]", "b"], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"p", " ", 
        SqrtBox[
         RowBox[{"w", "+", "b"}]]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "p"}], ")"}], 
        SqrtBox[
         RowBox[{"w", "-", "b"}]]}]}], ")"}]}], "\[Equal]", "0"}], ",", "b"}],
   "]"}]], "Input",
 CellChangeTimes->{{3.813942421702326*^9, 3.813942475364214*^9}},
 CellLabel->"In[98]:=",ExpressionUUID->"4e5a58b1-502c-4878-a105-b1583f8ebacf"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{"b", "\[Rule]", 
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "1"}], "+", 
        RowBox[{"2", " ", "p"}]}], ")"}], " ", "w"}], 
     RowBox[{"1", "-", 
      RowBox[{"2", " ", "p"}], "+", 
      RowBox[{"2", " ", 
       SuperscriptBox["p", "2"]}]}]]}], "}"}], "}"}]], "Output",
